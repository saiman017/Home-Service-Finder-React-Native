{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "app/(serviceProvider)/paymentcanceltsx",
    "node_modules/expo-modules-core/**/*.ts",
    "node_modules/react-native-dotenv/**/*.ts"
  ]
}
